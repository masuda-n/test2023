COLUMN TABLE "CUSTOMERS" (
  "CUSTOMERID" INTEGER,
  "CITYID" INTEGER,
  "COUNTRYID" INTEGER,
  "REGIONID" INTEGER,
  "CAMPANYID" INTEGER,
  "CUSTOMER" NVARCHAR(50),
  "CITYNAME" NVARCHAR(50),
  "COUNTRYNAME" NVARCHAR(50),
  "REGIONNAME" NVARCHAR(50),
  "CAMPANYNAME" NVARCHAR(50)
  PRIMARY KEY ("CUSTOMERID")
)